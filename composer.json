{
    "name": "saade/laravel-code-standards",
    "description": "My personal coding standards for a Laravel project.",
    "keywords": [
        "Saade",
        "laravel",
        "laravel-code-standards"
    ],
    "homepage": "https://github.com/saade/laravel-code-standards",
    "license": "MIT",
    "authors": [
        {
            "name": "Saade",
            "email": "saade@outlook.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^12.2.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^8.1.1||^7.10.0"
    },
    "autoload": {
        "psr-4": {
            "Saade\\LaravelCodeStandards\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saade\\LaravelCodeStandards\\LaravelCodeStandardsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
